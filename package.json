{
  "name": "landingpage",
  "version": "0.1.0",
  "description": "Landingpage for OFFICEMILL WEB",
  "author": "wanosoft",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "run-p start:*",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "clean": "rm -rf node_modules && pnpm install",
    "lint-staged": "NODE_OPTIONS=--max_old_space_size=4096 lint-staged",
    "lint:typecheck": "tsc -p . --noEmit",
    "lint:typecheck:staged": "tsc -p . --noEmit && echo #",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lint": "NODE_OPTIONS=--max_old_space_size=4096 pnpm lint:typecheck && pnpm lint:eslint && pnpm lint:prettier",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "pnpm lint:prettier --write",
    "fix": "pnpm lint:typecheck && pnpm fix:eslint && pnpm fix:prettier"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "framer-motion": "12.16.0",
    "husky": "9.1.5",
    "lint-staged": "15.2.9",
    "next": "14.2.7",
    "react": "18",
    "react-dom": "18"
  },
  "devDependencies": {
    "@types/node": "20",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.7",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-unused-imports": "4.1.3",
    "npm-run-all": "4.1.5",
    "postcss": "8",
    "tailwindcss": "3.4.1",
    "typescript": "5"
  }
}
